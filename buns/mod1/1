public class dz1 {

    public static void main(String[] args) {
        int totalforthree = 0;
        int totalforfive = 0;

        int total =0;

        for(int temp =0; temp < 1000 ; temp++){
            if(temp % 5 == 0){
                totalforthree += temp;
            }else if(temp % 3 == 0){
                totalforthree += temp;
            }
        }


        total = totalforfive + totalforthree;



        System.out.println(total);

    }

}
