public class dz2 {

    public static void main(String[] args) {
        int[][] x = { {20, 34, 2}, {9, 12, 18}, {3, 4, 5} };

        int i = x[0][0];

        for (int[] row : x) {
            for (int elem : row) {
                if (elem < i) {
                    i = elem;
                }
            }
        }

        System.out.println(i);
    }
}
